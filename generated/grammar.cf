Formula -> Expr ::= expr
Expr -> Expr OpExpr Term ::= sum
Expr -> Term ::= sum_unary
Term -> Term OpProduct Factor ::= product
Term -> Factor ::= product_unary
Factor -> '(' Expr ')' ::= parentheses
Factor -> Num ::= num_factor
Num -> "^[0-9]+$" ::= number
OpExpr -> '+' | '-' ::= operator_expr
OpProduct -> '*' | '/' ::= operator_prod